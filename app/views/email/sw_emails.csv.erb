<%=
  CSV.generate(headers: true) do |csv|
    csv << [:email, :stev_id, :stev_class, :name, :instance]

    Volunteer.all.each do |volunteer|
      csv << [
          volunteer.email,
          volunteer.id,
          volunteer.class.name,
          volunteer.full_name,
          Settings.instance_slug
      ]
    end
  end
%>
