<%=
  CSV.generate(headers: true) do |csv|
    csv << [:email, :stev_id, :stev_class, :name, 'Foreign Key']

    Volunteer.all.each do |volunteer|
      csv << [
          volunteer.email,
          volunteer.id,
          volunteer.class.name,
          volunteer.full_name,
          (Settings.instance_slug ? (Settings.instance_slug + ' ') : '') + volunteer.class.name + ' ' + volunteer.id.to_s
      ]
    end
  end
%>
