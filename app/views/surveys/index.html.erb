<h2>List of Surveys</h2>

<% Rails.application.routes.routes.each do |route| %>
  <% if route.defaults[:controller] == 'surveys' and route.defaults[:action] != 'index' %>
    <a href="<%= url_for(controller: route.defaults[:controller],
                action: route.defaults[:action],
                only_path: true) %>" class="btn btn-default btn-lg">
      <%= route.defaults[:action] %>
    </a>
  <% end %>
  <!--  <a href="<%#= url_for route %>"><%#= route.defaults[:controller] %></a><br>-->
<% end %>